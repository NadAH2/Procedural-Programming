PROCEDURE dot_product(VAR ps: FLOAT, V1,V2 : ARRAY_OF FLOAT[5])
VAR
    i:INTEGER;
    
BEGIN
    FOR i FROM 0 TO 4 STEP 1 DO
        
            ps:=ps+V1[i]*V2[i];
    END_FOR
    
    
END
ALGORITHM ProcesureAlgo
VAR
    R : ARRAY_OF FLOAT[4][5];
    i: INTEGER:=0;
BEGIN
    FOR i FROM 0 TO 3 STEP 2 DO
            IF (dot_product(R[i],R[i+1],ps)=0) THEN
                Write("the vectors are orthogonals", R[i],R[i+1])
            END_IF
        END_FOR
    END_FOR
END